import pandas as pd


file_path = "lab5.xlsx"


df = pd.read_excel(file_path, header=None, dtype=str)
df = df.drop(columns=list(range(25, 33)) + [3, 5, 10, 15, 20])
df = df.head(15)


columns_to_merge = list(range(6, 40))
existing_cols = [c for c in columns_to_merge if c in df.columns]


df[20] = (
    df[existing_cols]
    .fillna('')
    .astype(str)
    .agg(''.join, axis=1)
)
# df = df.drop(columns=existing_cols)
df = df.dropna()
        
