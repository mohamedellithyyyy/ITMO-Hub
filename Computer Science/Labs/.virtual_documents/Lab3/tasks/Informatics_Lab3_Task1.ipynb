import re
def solve(text):
    pattern = r'([А-Я][а-я]+(?:-[А-Я][а-я]+)?)\s+[А-Я]\.[А-Я]\.'
    surnames = re.findall(pattern, text)
    surnames = list(set(surnames))
    surnames.sort()
    return surnames


